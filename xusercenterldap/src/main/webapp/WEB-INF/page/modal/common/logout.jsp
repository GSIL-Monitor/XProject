<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">确认框</h4>
</div>
<div class="modal-body">
    <p class="confirm">是否确认退出用户中心？</p>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default btn-xs" data-dismiss="modal"><i class="icon-remove"></i> 取消</button>
    <button id="logout_confirm_btn" type="button" class="btn btn-danger btn-xs"><i class="icon-ok"></i> 确定</button>
</div>